<p id="notice"><%= notice %></p>

<h3><center>Listing Students</center></h3>


<div class="col-md-6 col-md-offset-3">
  <div class="table-responsive">
    <table class="table table-bordered">
      <thead>
        <tr class="active">
          <th>Regno</th>
          <th>Name</th>
          <th colspan="3"></th>
        </tr>
      </thead>

      <tbody>
        <% @student_list.each do |student| %>
          <tr>
             <td><%= student.id %></td>
            <td><%= student.regno %></td>
            <td><%= student.name %></td>
            <td><%= link_to 'Show', viewtc_path(student.id) %></td>

            <!-- <td> link_to 'Show', viewtc %> -->
             <!-- link_to 'Edit', edit_student_detail_path(student_detail) %>  for edit-->
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
 
</div>
 
