<p id="notice"><%= notice %></p>

<h1>Listing Student Details</h1>

<table>
  <thead>
    <tr>
      <th>Regno</th>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @student_details.each do |student_detail| %>
      <tr>
        <td><%= student_detail.regno %></td>
        <td><%= student_detail.name %></td>
        <td><%= link_to 'Show', student_detail %></td>
     
      </tr>
    <% end %>
  </tbody>
</table>

<br>